__kernel void progressiveArraySum (__global float* inputA, __global float* outputB)
{
	const int x = get_global_id(0);

	// B(i)=sum(j=1 to i) A(j) for i = 1 ... N
	const int id = x;

	float tmp = 0.0f;
	for(unsigned int i = 0; i < id; ++i){ 
		tmp += inputA[i];
	}
	
	outputB[id] = tmp + inputA[id];
	
}
